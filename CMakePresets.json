{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20
    },
    "configurePresets": [
        {
            "name": "build_nrf52840dk_nrf52840",
            "displayName": "Build for nRF52840 DK NRF52840",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build_nrf52840dk_nrf52840",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "nrf52840dk_nrf52840"
            }
        },
        {
            "name": "build_nrf52840dongle_nrf52840",
            "displayName": "Build for nRF52840 Dongle NRF52840",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build_nrf52840dongle_nrf52840",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "nrf52840dongle_nrf52840"
            }
        },
        {
            "name": "build_supermini_uf2",
            "displayName": "Build for supermini_uf2",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build_supermini_uf2",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "supermini_uf2"
            }
        },
        {
            "name": "build_etee_dongle_uf2",
            "displayName": "Build for etee_dongle_uf2",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build_etee_dongle_uf2",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "etee_dongle_uf2"
            }
        }
    ]
}